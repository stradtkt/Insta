<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Insta</title>
        <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-9ralMzdK1QYsk4yBY680hmsb4/hJ98xK3w0TIaJ3ll4POWpWUYaA2bRjGGujGT8w" crossorigin="anonymous">
        <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/jasny-bootstrap/3.1.3/css/jasny-bootstrap.min.css">
        <link rel="stylesheet" href="~/theme/assets/css/bootstrap.min.css">
        <link rel="stylesheet" href="~/theme/assets/css/now-ui-kit.min.css">
        <link rel='stylesheet' href='~/gallery/source/unitegallery/css/unite-gallery.css' type='text/css' />
        <link rel='stylesheet' href='~/gallery/source/unitegallery/themes/default/ug-theme-default.css' type='text/css' />
        <link rel="stylesheet" href="~/css/site.css">
</head>
<body>
<nav class="navbar navbar-expand-lg fixed-top bg-info">
    <a class="navbar-brand">Insta</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
     <div class="collapse navbar-collapse" id="navbarNavDropdown">
         @if(ViewBag.user != null)
         {
            <ul class="navbar-nav mx-auto">
                <li class="nav-item">
                    <a class="nav-link" asp-action="Dashboard" asp-controller="Insta">InstaFeed</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" asp-action="Users" asp-controller="Friend">Users</a>
                </li>
            </ul>
            <ul class="navbar-nav ml-auto">
                <li class="nav-item">
                    <a asp-action="Logout" asp-controller="Home" class="nav-link">Logout</a>
                </li>
            </ul>
         }
         else
         {
             <ul class="navbar-nav">
                 <li class="nav-item">
                     <a asp-action="Register" asp-controller="Home" class="nav-link">Register</a>
                 </li>
                 <li class="nav-item">
                     <a asp-action="Login" asp-controller="Home" class="nav-link">Login</a>
                 </li>
             </ul>
         }
    </div>
</nav>
        @RenderBody()
        <footer class=" mt-5">
            <div class="container">
                <div class="row">
                    <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4">
                        <p class="mt-5 text-white mx-auto">&copy; Kevin Stradtman | Insta - 2018</p>
                    </div>
                    <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4"></div>
                    <div class="col-4 col-sm-4 col-md-4 col-lg-4 col-xl-4"></div>
                </div>
            </div>
        </footer>
        <script src="~/theme/assets/js/core/jquery.min.js"></script>
        <script src="~/theme/assets/js/core/bootstrap.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/jasny-bootstrap/3.1.3/js/jasny-bootstrap.min.js"></script>
        <script src="~/theme/assets/js/core/popper.min.js"></script>
        <script src="~/theme/assets/js/now-ui-kit.min.js"></script>
        <script src="~/js/site.js"></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-common-libraries.js'></script>	
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-functions.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-thumbsgeneral.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-thumbsstrip.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-touchthumbs.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-panelsbase.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-strippanel.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-gridpanel.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-thumbsgrid.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-tiles.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-tiledesign.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-avia.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-slider.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-sliderassets.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-touchslider.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-zoomslider.js'></script>	
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-video.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-gallery.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-lightbox.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-carousel.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/js/ug-api.js'></script>
        <script type='text/javascript' src='~/gallery/source/unitegallery/themes/default/ug-theme-default.js'></script>
    	<script type="text/javascript">

		jQuery(document).ready(function(){

			jQuery("#gallery").unitegallery();

		});
		
	</script>
    @RenderSection("Scripts", required: false)
</body>
</html>
